package numericalData;
import java.util.Scanner;
public class Exercise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanMe=new Scanner(System.in);
		
		String name;
		int age;
		double height;
		boolean married;
		char grade;
		
		System.out.print("Enter your name,age,height,married,grade for Math : ");
		name=scanMe.next();
		age=scanMe.nextInt();
		height=scanMe.nextDouble();
		married=scanMe.nextBoolean();
		grade=scanMe.next().charAt(0);
		
		System.out.println("Name : " + name);
		System.out.println("Age : " + age) ;
		System.out.println("Height : " + height);
		System.out.println("Status : " + married);
		System.out.println("Grade for Math : " + grade);
				

	}

}
