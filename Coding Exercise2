public class Exercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double bookPrice=23.85;
		double discount=0.1*bookPrice;
		double newPrice=bookPrice-discount;
		
		System.out.printf ("The price of the book is RM%.2f" , newPrice);
	}

}
